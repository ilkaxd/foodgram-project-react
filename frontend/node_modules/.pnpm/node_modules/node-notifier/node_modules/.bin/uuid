#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/_Work/foodgram-project-react/frontend/node_modules/.pnpm/uuid@8.3.2/node_modules/uuid/dist/bin/node_modules:/mnt/c/_Work/foodgram-project-react/frontend/node_modules/.pnpm/uuid@8.3.2/node_modules/uuid/dist/node_modules:/mnt/c/_Work/foodgram-project-react/frontend/node_modules/.pnpm/uuid@8.3.2/node_modules/uuid/node_modules:/mnt/c/_Work/foodgram-project-react/frontend/node_modules/.pnpm/uuid@8.3.2/node_modules:/mnt/c/_Work/foodgram-project-react/frontend/node_modules/.pnpm/node_modules:/mnt/c/_Work/foodgram-project-react/frontend/node_modules:/mnt/c/_Work/foodgram-project-react/node_modules:/mnt/c/_Work/node_modules:/mnt/c/node_modules:/mnt/c/_Work/foodgram-project-react/frontend/node_modules/.pnpm/node-notifier@8.0.2/node_modules/node-notifier/node_modules:/mnt/c/_Work/foodgram-project-react/frontend/node_modules/.pnpm/node-notifier@8.0.2/node_modules"
else
  export NODE_PATH="$NODE_PATH:/mnt/c/_Work/foodgram-project-react/frontend/node_modules/.pnpm/uuid@8.3.2/node_modules/uuid/dist/bin/node_modules:/mnt/c/_Work/foodgram-project-react/frontend/node_modules/.pnpm/uuid@8.3.2/node_modules/uuid/dist/node_modules:/mnt/c/_Work/foodgram-project-react/frontend/node_modules/.pnpm/uuid@8.3.2/node_modules/uuid/node_modules:/mnt/c/_Work/foodgram-project-react/frontend/node_modules/.pnpm/uuid@8.3.2/node_modules:/mnt/c/_Work/foodgram-project-react/frontend/node_modules/.pnpm/node_modules:/mnt/c/_Work/foodgram-project-react/frontend/node_modules:/mnt/c/_Work/foodgram-project-react/node_modules:/mnt/c/_Work/node_modules:/mnt/c/node_modules:/mnt/c/_Work/foodgram-project-react/frontend/node_modules/.pnpm/node-notifier@8.0.2/node_modules/node-notifier/node_modules:/mnt/c/_Work/foodgram-project-react/frontend/node_modules/.pnpm/node-notifier@8.0.2/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../uuid@8.3.2/node_modules/uuid/dist/bin/uuid" "$@"
else
  exec node  "$basedir/../../../../../uuid@8.3.2/node_modules/uuid/dist/bin/uuid" "$@"
fi
