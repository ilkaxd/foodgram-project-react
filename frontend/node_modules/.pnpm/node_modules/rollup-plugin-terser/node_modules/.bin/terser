#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/_Work/foodgram-project-react/frontend/node_modules/.pnpm/terser@4.8.0/node_modules/terser/bin/node_modules:/mnt/c/_Work/foodgram-project-react/frontend/node_modules/.pnpm/terser@4.8.0/node_modules/terser/node_modules:/mnt/c/_Work/foodgram-project-react/frontend/node_modules/.pnpm/terser@4.8.0/node_modules:/mnt/c/_Work/foodgram-project-react/frontend/node_modules/.pnpm/node_modules:/mnt/c/_Work/foodgram-project-react/frontend/node_modules:/mnt/c/_Work/foodgram-project-react/node_modules:/mnt/c/_Work/node_modules:/mnt/c/node_modules:/mnt/c/_Work/foodgram-project-react/frontend/node_modules/.pnpm/rollup-plugin-terser@5.3.1_rollup@1.32.1/node_modules/rollup-plugin-terser/node_modules:/mnt/c/_Work/foodgram-project-react/frontend/node_modules/.pnpm/rollup-plugin-terser@5.3.1_rollup@1.32.1/node_modules"
else
  export NODE_PATH="$NODE_PATH:/mnt/c/_Work/foodgram-project-react/frontend/node_modules/.pnpm/terser@4.8.0/node_modules/terser/bin/node_modules:/mnt/c/_Work/foodgram-project-react/frontend/node_modules/.pnpm/terser@4.8.0/node_modules/terser/node_modules:/mnt/c/_Work/foodgram-project-react/frontend/node_modules/.pnpm/terser@4.8.0/node_modules:/mnt/c/_Work/foodgram-project-react/frontend/node_modules/.pnpm/node_modules:/mnt/c/_Work/foodgram-project-react/frontend/node_modules:/mnt/c/_Work/foodgram-project-react/node_modules:/mnt/c/_Work/node_modules:/mnt/c/node_modules:/mnt/c/_Work/foodgram-project-react/frontend/node_modules/.pnpm/rollup-plugin-terser@5.3.1_rollup@1.32.1/node_modules/rollup-plugin-terser/node_modules:/mnt/c/_Work/foodgram-project-react/frontend/node_modules/.pnpm/rollup-plugin-terser@5.3.1_rollup@1.32.1/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../terser@4.8.0/node_modules/terser/bin/terser" "$@"
else
  exec node  "$basedir/../../../../../terser@4.8.0/node_modules/terser/bin/terser" "$@"
fi
