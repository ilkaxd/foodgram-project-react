#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/_Work/foodgram-project-react/frontend/node_modules/.pnpm/browserslist@4.16.8/node_modules/browserslist/node_modules:/mnt/c/_Work/foodgram-project-react/frontend/node_modules/.pnpm/browserslist@4.16.8/node_modules:/mnt/c/_Work/foodgram-project-react/frontend/node_modules/.pnpm/node_modules:/mnt/c/_Work/foodgram-project-react/frontend/node_modules:/mnt/c/_Work/foodgram-project-react/node_modules:/mnt/c/_Work/node_modules:/mnt/c/node_modules:/mnt/c/_Work/foodgram-project-react/frontend/node_modules/.pnpm/@babel+helper-compilation-targets@7.15.0_@babel+core@7.12.3/node_modules/@babel/helper-compilation-targets/node_modules:/mnt/c/_Work/foodgram-project-react/frontend/node_modules/.pnpm/@babel+helper-compilation-targets@7.15.0_@babel+core@7.12.3/node_modules/@babel/node_modules:/mnt/c/_Work/foodgram-project-react/frontend/node_modules/.pnpm/@babel+helper-compilation-targets@7.15.0_@babel+core@7.12.3/node_modules"
else
  export NODE_PATH="$NODE_PATH:/mnt/c/_Work/foodgram-project-react/frontend/node_modules/.pnpm/browserslist@4.16.8/node_modules/browserslist/node_modules:/mnt/c/_Work/foodgram-project-react/frontend/node_modules/.pnpm/browserslist@4.16.8/node_modules:/mnt/c/_Work/foodgram-project-react/frontend/node_modules/.pnpm/node_modules:/mnt/c/_Work/foodgram-project-react/frontend/node_modules:/mnt/c/_Work/foodgram-project-react/node_modules:/mnt/c/_Work/node_modules:/mnt/c/node_modules:/mnt/c/_Work/foodgram-project-react/frontend/node_modules/.pnpm/@babel+helper-compilation-targets@7.15.0_@babel+core@7.12.3/node_modules/@babel/helper-compilation-targets/node_modules:/mnt/c/_Work/foodgram-project-react/frontend/node_modules/.pnpm/@babel+helper-compilation-targets@7.15.0_@babel+core@7.12.3/node_modules/@babel/node_modules:/mnt/c/_Work/foodgram-project-react/frontend/node_modules/.pnpm/@babel+helper-compilation-targets@7.15.0_@babel+core@7.12.3/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../../browserslist@4.16.8/node_modules/browserslist/cli.js" "$@"
else
  exec node  "$basedir/../../../../../../browserslist@4.16.8/node_modules/browserslist/cli.js" "$@"
fi
