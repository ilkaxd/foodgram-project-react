#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/_Work/foodgram-project-react/frontend/node_modules/.pnpm/import-local@3.0.2/node_modules/import-local/fixtures/node_modules:/mnt/c/_Work/foodgram-project-react/frontend/node_modules/.pnpm/import-local@3.0.2/node_modules/import-local/node_modules:/mnt/c/_Work/foodgram-project-react/frontend/node_modules/.pnpm/import-local@3.0.2/node_modules:/mnt/c/_Work/foodgram-project-react/frontend/node_modules/.pnpm/node_modules:/mnt/c/_Work/foodgram-project-react/frontend/node_modules:/mnt/c/_Work/foodgram-project-react/node_modules:/mnt/c/_Work/node_modules:/mnt/c/node_modules:/mnt/c/_Work/foodgram-project-react/frontend/node_modules/.pnpm/jest@26.6.0/node_modules/jest/node_modules:/mnt/c/_Work/foodgram-project-react/frontend/node_modules/.pnpm/jest@26.6.0/node_modules"
else
  export NODE_PATH="$NODE_PATH:/mnt/c/_Work/foodgram-project-react/frontend/node_modules/.pnpm/import-local@3.0.2/node_modules/import-local/fixtures/node_modules:/mnt/c/_Work/foodgram-project-react/frontend/node_modules/.pnpm/import-local@3.0.2/node_modules/import-local/node_modules:/mnt/c/_Work/foodgram-project-react/frontend/node_modules/.pnpm/import-local@3.0.2/node_modules:/mnt/c/_Work/foodgram-project-react/frontend/node_modules/.pnpm/node_modules:/mnt/c/_Work/foodgram-project-react/frontend/node_modules:/mnt/c/_Work/foodgram-project-react/node_modules:/mnt/c/_Work/node_modules:/mnt/c/node_modules:/mnt/c/_Work/foodgram-project-react/frontend/node_modules/.pnpm/jest@26.6.0/node_modules/jest/node_modules:/mnt/c/_Work/foodgram-project-react/frontend/node_modules/.pnpm/jest@26.6.0/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../import-local@3.0.2/node_modules/import-local/fixtures/cli.js" "$@"
else
  exec node  "$basedir/../../../../../import-local@3.0.2/node_modules/import-local/fixtures/cli.js" "$@"
fi
