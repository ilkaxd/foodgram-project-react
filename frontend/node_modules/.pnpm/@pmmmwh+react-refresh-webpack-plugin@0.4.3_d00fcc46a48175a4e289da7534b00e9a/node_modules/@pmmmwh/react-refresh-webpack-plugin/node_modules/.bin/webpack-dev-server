if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../../webpack-dev-server@3.11.0_webpack@4.44.2/node_modules/webpack-dev-server/bin/webpack-dev-server.js" "$@"
