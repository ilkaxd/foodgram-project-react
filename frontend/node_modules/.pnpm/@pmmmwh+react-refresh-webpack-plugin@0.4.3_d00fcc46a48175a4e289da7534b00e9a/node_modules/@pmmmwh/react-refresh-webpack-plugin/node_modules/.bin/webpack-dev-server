  export NODE_PATH="/mnt/c/_Work/foodgram-project-react/frontend/node_modules/.pnpm/webpack-dev-server@3.11.0_webpack@4.44.2/node_modules/webpack-dev-server/bin/node_modules:/mnt/c/_Work/foodgram-project-react/frontend/node_modules/.pnpm/webpack-dev-server@3.11.0_webpack@4.44.2/node_modules/webpack-dev-server/node_modules:/mnt/c/_Work/foodgram-project-react/frontend/node_modules/.pnpm/webpack-dev-server@3.11.0_webpack@4.44.2/node_modules:/mnt/c/_Work/foodgram-project-react/frontend/node_modules/.pnpm/node_modules:/mnt/c/_Work/foodgram-project-react/frontend/node_modules:/mnt/c/_Work/foodgram-project-react/node_modules:/mnt/c/_Work/node_modules:/mnt/c/node_modules:/mnt/c/_Work/foodgram-project-react/frontend/node_modules/.pnpm/@pmmmwh+react-refresh-webpack-plugin@0.4.3_d00fcc46a48175a4e289da7534b00e9a/node_modules/@pmmmwh/react-refresh-webpack-plugin/node_modules:/mnt/c/_Work/foodgram-project-react/frontend/node_modules/.pnpm/@pmmmwh+react-refresh-webpack-plugin@0.4.3_d00fcc46a48175a4e289da7534b00e9a/node_modules/@pmmmwh/node_modules:/mnt/c/_Work/foodgram-project-react/frontend/node_modules/.pnpm/@pmmmwh+react-refresh-webpack-plugin@0.4.3_d00fcc46a48175a4e289da7534b00e9a/node_modules"
else
  export NODE_PATH="$NODE_PATH:/mnt/c/_Work/foodgram-project-react/frontend/node_modules/.pnpm/webpack-dev-server@3.11.0_webpack@4.44.2/node_modules/webpack-dev-server/bin/node_modules:/mnt/c/_Work/foodgram-project-react/frontend/node_modules/.pnpm/webpack-dev-server@3.11.0_webpack@4.44.2/node_modules/webpack-dev-server/node_modules:/mnt/c/_Work/foodgram-project-react/frontend/node_modules/.pnpm/webpack-dev-server@3.11.0_webpack@4.44.2/node_modules:/mnt/c/_Work/foodgram-project-react/frontend/node_modules/.pnpm/node_modules:/mnt/c/_Work/foodgram-project-react/frontend/node_modules:/mnt/c/_Work/foodgram-project-react/node_modules:/mnt/c/_Work/node_modules:/mnt/c/node_modules:/mnt/c/_Work/foodgram-project-react/frontend/node_modules/.pnpm/@pmmmwh+react-refresh-webpack-plugin@0.4.3_d00fcc46a48175a4e289da7534b00e9a/node_modules/@pmmmwh/react-refresh-webpack-plugin/node_modules:/mnt/c/_Work/foodgram-project-react/frontend/node_modules/.pnpm/@pmmmwh+react-refresh-webpack-plugin@0.4.3_d00fcc46a48175a4e289da7534b00e9a/node_modules/@pmmmwh/node_modules:/mnt/c/_Work/foodgram-project-react/frontend/node_modules/.pnpm/@pmmmwh+react-refresh-webpack-plugin@0.4.3_d00fcc46a48175a4e289da7534b00e9a/node_modules"
