#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/_Work/foodgram-project-react/frontend/node_modules/.pnpm/acorn@7.4.1/node_modules/acorn/bin/node_modules:/mnt/c/_Work/foodgram-project-react/frontend/node_modules/.pnpm/acorn@7.4.1/node_modules/acorn/node_modules:/mnt/c/_Work/foodgram-project-react/frontend/node_modules/.pnpm/acorn@7.4.1/node_modules:/mnt/c/_Work/foodgram-project-react/frontend/node_modules/.pnpm/node_modules:/mnt/c/_Work/foodgram-project-react/frontend/node_modules:/mnt/c/_Work/foodgram-project-react/node_modules:/mnt/c/_Work/node_modules:/mnt/c/node_modules:/mnt/c/_Work/foodgram-project-react/frontend/node_modules/.pnpm/rollup@1.32.1/node_modules/rollup/node_modules:/mnt/c/_Work/foodgram-project-react/frontend/node_modules/.pnpm/rollup@1.32.1/node_modules"
else
  export NODE_PATH="$NODE_PATH:/mnt/c/_Work/foodgram-project-react/frontend/node_modules/.pnpm/acorn@7.4.1/node_modules/acorn/bin/node_modules:/mnt/c/_Work/foodgram-project-react/frontend/node_modules/.pnpm/acorn@7.4.1/node_modules/acorn/node_modules:/mnt/c/_Work/foodgram-project-react/frontend/node_modules/.pnpm/acorn@7.4.1/node_modules:/mnt/c/_Work/foodgram-project-react/frontend/node_modules/.pnpm/node_modules:/mnt/c/_Work/foodgram-project-react/frontend/node_modules:/mnt/c/_Work/foodgram-project-react/node_modules:/mnt/c/_Work/node_modules:/mnt/c/node_modules:/mnt/c/_Work/foodgram-project-react/frontend/node_modules/.pnpm/rollup@1.32.1/node_modules/rollup/node_modules:/mnt/c/_Work/foodgram-project-react/frontend/node_modules/.pnpm/rollup@1.32.1/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../acorn@7.4.1/node_modules/acorn/bin/acorn" "$@"
else
  exec node  "$basedir/../../../../../acorn@7.4.1/node_modules/acorn/bin/acorn" "$@"
fi
